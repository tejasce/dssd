TOOLS := /usr

CC := $(TOOLS)/bin/gcc
FLEX := flex
BISON := bison

CFLAGS := -m64
CFLAGS += -g -fno-omit-frame-pointer
CFLAGS += -Wvla
CFLAGS += -O3
CFLAGS += -std=gnu99 -D_GNU_SOURCE -D__USE_LARGEFILE64
CFLAGS += -I. -I../../include

LDLIBS :=
